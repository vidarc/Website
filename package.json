{
  "name": "Website",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-server": "webpack --config webpack.config.server.js --progress --colors",
    "build-semantic": "gulp --gulpfile ./semantic/gulpfile.js build",
    "test": "react-scripts test --env=jsdom --setupFiles raf/polyfill --setupFiles whatwg-fetch",
    "coverage": "yarn coverage:run && yarn coverage:submit && yarn coverage:cleanup",
    "coverage:run": "react-scripts test --env=jsdom --coverage --setupFiles raf/polyfill --setupFiles whatwg-fetch",
    "coverage:submit": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "coverage:cleanup": "rm -rf ./coverage",
    "eject": "react-scripts eject",
    "flow": "flow"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/style/semantic/**/*.js",
      "!src/stories/*.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "codacy-coverage": "^2.0.3",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.66.0",
    "nock": "^9.0.22",
    "raf": "^3.4.0",
    "react-scripts": "^1.0.1",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "semantic-ui": "^2.2.13",
    "webpack": "3.8.1",
    "whatwg-fetch": "^2.0.3"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "dotenv": "^5.0.0",
    "express": "^4.14.0",
    "handlebars": "^4.0.6",
    "jwt-decode": "^2.1.0",
    "markdown-it": "^8.2.1",
    "moment": "^2.17.1",
    "mongodb": "^3.0.3",
    "morgan": "^1.7.0",
    "node-fetch": "^2.0.0",
    "nodemailer": "^4.0.1",
    "prop-types": "^15.5.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.78.2"
  }
}
